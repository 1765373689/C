97

  代码实现：
int N = 0;
	//0表示不能被整除是素数，1表示能被整除不是素数
	int isPrime = 0;
	scanf("%d", &N);
	for (int i = 2; i <= N; i++) {
		for (int j = 2; j < i; j++) {
			if (i % j == 0 && i != j)
				isPrime = 1;
		}
		if (isPrime == 0)printf("%d\n", i);
		isPrime = 0;
	}
